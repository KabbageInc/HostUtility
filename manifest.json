{
    "manifest_version":2,

    "background":{
        "scripts":["js/jquery.min.js", "lib.js", "bg.js"]
    },

    "name":"Kabbage Host Utility",
    "description":"Modify host headers for our QA environments",
    "version":"1.3",

    "icons":{
        "16":"kabbage.png",
        "48":"kabbage-48.png",
        "128":"kabbage-128.png"
    },

    "permissions":[ "tabs", "background", "webRequest", "storage", "webRequestBlocking", "<all_urls>"],
    "browser_action":{
        "default_icon":"kabbage.png",
        "default_popup":"popup.html"
    }
}